@model IEnumerable<Project.Models.ControlAcceso>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StrIdDestino)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrPlaca)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrConductor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DtFechaIngreso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DtFechaSalida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntTopeMensual)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntAcumulado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DtFechaValidez)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrTipoTarjeta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntIdContrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrIdMina)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrCifProveedor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrRfid)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StrIdDestino)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrPlaca)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrConductor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DtFechaIngreso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DtFechaSalida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntTopeMensual)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntAcumulado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DtFechaValidez)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrTipoTarjeta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntIdContrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrIdMina)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrCifProveedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrRfid)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IntIdControl">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IntIdControl">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IntIdControl">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
