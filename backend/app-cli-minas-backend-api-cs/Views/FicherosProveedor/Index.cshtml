@model IEnumerable<Project.Models.FicherosProveedor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StrNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntTopeMensual)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DtFechaHoraCarga)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrIdUsuario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StrNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntTopeMensual)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DtFechaHoraCarga)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrIdUsuario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.StrCif">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.StrCif">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.StrCif">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
